!--------------
! Xft
Xft.antialias: 1
Xft.autohint: 0
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba: rgb

!------------------------------------------------------------------------------
! u r x v t

! URxvt*geometry:         112x22
! URxvt*font:            xft:terminus:size=8
! URxvt*urlLauncher:         firefox
URxvt*scrollBar:        false
URxvt*scrollBar_right:        true
URxvt*scrollColor:        #000000

URxvt.font: xft:Hack:size=14
! resize font
URxvt.resize-font.smaller: M-S-Down
URxvt.resize-font.bigger: M-S-Up

! perl extensions
URxvt.perl-ext-common: default,resize-font
URxvt.perl-ext: default,url-select

! launch URLs
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/google-chrome-unstable
URxvt.url-select.underline: true
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1


